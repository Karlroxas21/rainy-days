					"name": "Register - Success",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test('authentication#register => success', () => {\r",
									"    pm.expect(pm.response).status(201)\r",
									"    const { name, email, username, first_name, middle_name, last_name } = pm.response.json()\r",
									"    pm.environment.set('user-name', name)\r",
									"    pm.environment.set('user-email', email.toLowerCase())\r",
									"    pm.environment.set('user-username', username.toLowerCase())\r",
									"    pm.environment.set('user-first_name', first_name)\r",
									"    pm.environment.set('user-middle_name', middle_name)\r",
									"    pm.environment.set('user-last-name', last_name)\r",
									"})"
								],
